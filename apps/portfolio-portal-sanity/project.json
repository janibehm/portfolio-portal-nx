{
  "name": "portfolio-portal-sanity",
  "projectType": "application",
  "root": "apps/portfolio-portal-sanity",
  "sourceRoot": "apps/portfolio-portal-sanity",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx sanity dev --port 3333",
        "cwd": "apps/portfolio-portal-sanity"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx sanity build",
        "cwd": "apps/portfolio-portal-sanity"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx sanity deploy",
        "cwd": "apps/portfolio-portal-sanity"
      }
    }
  },
  "tags": []
}
