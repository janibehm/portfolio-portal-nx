{
  "name": "portfolio-portal",
  "projectType": "application",
  "root": "apps/portfolio-portal",
  "sourceRoot": "apps/portfolio-portal",
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "portfolio-portal:build",
        "dev": true
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next build",
        "cwd": "apps/portfolio-portal"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next start",
        "cwd": "apps/portfolio-portal"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
